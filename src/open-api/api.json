{
  "swagger": "2.0",
  "info": {
    "description": "Klokasale backend server api.",
    "title": "Klokasale",
    "contact": {},
    "version": "2.0"
  },
  "host": "localhost:8080/",
  "basePath": "api",
  "paths": {
    "/v1/users/login": {
      "post": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["users"],
        "summary": "User Login",
        "parameters": [
          {
            "description": "Login Request Body",
            "name": "LoginRequest",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/user.UserLoginReq"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/common.SuccessResponse"
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/common.ErrorResponse"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/common.ErrorResponse"
            }
          }
        }
      }
    },
    "/v1/users/register": {
      "post": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["users"],
        "summary": "User Login",
        "parameters": [
          {
            "description": "Register Request Body",
            "name": "LoginRequest",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/user.UserRegisterReq"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/common.SuccessResponse"
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/common.ErrorResponse"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/common.ErrorResponse"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "common.ErrorResponse": {
      "type": "object",
      "properties": {
        "error": {
          "type": "object",
          "properties": {
            "code": {
              "description": "can be custom error if necessary",
              "type": "integer"
            },
            "message": {
              "type": "string"
            }
          }
        }
      }
    },
    "common.SuccessResponse": {
      "type": "object",
      "properties": {
        "data": {}
      }
    },
    "user.UserLoginReq": {
      "type": "object",
      "required": ["password", "username"],
      "properties": {
        "password": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      }
    },
    "user.UserRegisterReq": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "phone_number": {
          "type": "string"
        },
        "username": {
          "type": "string"
        }
      }
    }
  }
}
